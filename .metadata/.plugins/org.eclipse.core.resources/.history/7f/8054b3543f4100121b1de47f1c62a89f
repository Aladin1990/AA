package main;

public class GiftBuilder{
	StringBuilder giftOutPut = new StringBuilder();
	GiftBuilder()
	{
		
	}
	public StringBuilder getGiftOutPut() {
		return giftOutPut;
	}
	public void appendToGiftOutPut(String giftOutPut) {
		this.giftOutPut.append(giftOutPut);
	}
	public void deleteLastEntery(String lastEntery) {
		this.giftOutPut.replace(giftOutPut.indexOf(lastEntery), giftOutPut.length(),"");
	}
	

}
